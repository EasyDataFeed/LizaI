        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="CodeDialog" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="CodeTextBox" HorizontalAlignment="Left" Height="25" Margin="10,68,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="372"/>
        <Button Content="Ok" HorizontalAlignment="Left" Height="25" Margin="263,134,0,0" VerticalAlignment="Top" Width="119" Click="Button_Click"/>
        <Label x:Name="phoneNumber" Content="Phone number:" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top" Width="372"/>

    </Grid>
</Window>
